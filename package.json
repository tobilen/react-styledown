{
  "name": "react-styledown",
  "version": "0.0.24",
  "description": "Helper to pass a styled-components styling down to its child",
  "main": "dist/index.js",
  "repository": "https://github.com/tobilen/react-styledown.git",
  "author": "Tobias Lengsholz<tobias.lengsholz@invision.de>",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^16.9.19",
    "@types/styled-components": "4.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^3.0.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.1",
    "jest": "^25.1.0",
    "jest-styled-components": "^6.3.3",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.0.0",
    "styled-components": "^4.4.1",
    "ts-jest": "^25.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": ">=15.x.x",
    "react-dom": ">=15.x.x",
    "styled-components": ">=3.x.x"
  },
  "scripts": {
    "build": "tsc --p . --emitDeclarationOnly && rollup -c",
    "dev": "rollup -c -w",
    "lint": "prettier --check '**/*.{js,ts,tsx,json,md}' --single-quote --trailing-comma all",
    "format": "prettier --write '**/*.{js,ts,tsx,json,md}' --single-quote --trailing-comma all",
    "test": "lint && jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
